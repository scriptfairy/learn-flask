  "devDependencies": {
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
