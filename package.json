{
  "name": "learn-flask",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently \"npm:server\" \"npm:client:js\" \"npm:client:css\"",
    "server": "flask --app src/server/main.py --debug run",
    "client:js": "esbuild src/client/main.tsx --bundle --outfile=build/main.js --watch",
    "client:css": "npx tailwindcss -i ./src/client/main.css -o ./build/main.css --watch",
    "types:check": "tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "@types/react-query": "^1.2.9",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.13.0",
    "zod": "^3.21.4"
  }
}
